class Solution {
    public int missingNumber(int[] nums) {
       
        int ssum=0;
        int sum=0;
        int range= nums.length;
        for( int i=0; i<range+1; i++)
        {
             ssum= ssum+i;
        }
        for( int i=0; i<nums.length; i++)
        {
             sum= sum+nums[i];
        }
        int req= ssum-sum;
       

        return req;
    }
}
